{
  "name": "web-admin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:serve": "node server.cjs",
    "dev:all": "npm-run-all -p dev dev:serve",
    "build": "vite build",
    "preview": "vite preview --host 127.0.0.1 --port 4173",
    "serve": "node server.cjs",
    "package:win": "node scripts/package-win.cjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report",
    "e2e:codegen": "playwright codegen http://localhost:4173",
    "test:cleanup": "node scripts/cleanup-e2e.cjs",
    "test:e2e:all": "playwright test && node scripts/cleanup-e2e.cjs",
    "test:e2e:chrome-devtools": "node scripts/run-chrome-devtools-tests.cjs",
    "test:e2e:chrome-devtools:infra": "node scripts/run-chrome-devtools-tests.cjs infrastructure",
    "test:e2e:chrome-devtools:advanced": "node scripts/run-chrome-devtools-tests.cjs advanced",
    "test:e2e:chrome-devtools:business": "node scripts/run-chrome-devtools-tests.cjs business",
    "test:e2e:chrome-devtools:headed": "HEADED=true node scripts/run-chrome-devtools-tests.cjs",
    "test:e2e:chrome-mcp": "playwright test --config=chrome-mcp.config.ts",
    "test:e2e:chrome-mcp:headless": "playwright test --config=chrome-mcp.config.ts --headed=false",
    "test:e2e:chrome-mcp:business": "playwright test --config=chrome-mcp.config.ts chrome-mcp-business-workflows.spec.ts",
    "test:e2e:chrome-mcp:data": "playwright test --config=chrome-mcp.config.ts chrome-mcp-data-management.spec.ts",
    "test:e2e:chrome-mcp:performance": "playwright test --config=chrome-mcp.config.ts chrome-mcp-performance.spec.ts",
    "test:e2e:chrome-mcp:all": "playwright test --config=chrome-mcp.config.ts",
    "test:e2e:chrome-mcp:debug": "PWDEBUG=1 playwright test --config=chrome-mcp.config.ts",
    "test:e2e:chrome-mcp:ui": "playwright test --config=chrome-mcp.config.ts --ui",
    "test:e2e:chrome-mcp:report": "playwright test --config=chrome-mcp.config.ts && playwright show-report",
    "test:e2e:chrome-mcp:mobile": "playwright test --config=chrome-mcp.config.ts --project=chrome-mcp-mobile",
    "test:e2e:chrome-mcp:tablet": "playwright test --config=chrome-mcp.config.ts --project=chrome-mcp-tablet",
    "test:e2e:chrome-mcp:cleanup": "node scripts/cleanup-chrome-mcp-tests.cjs",
    "test:e2e:chrome-mcp:setup": "node scripts/setup-chrome-mcp-environment.cjs",
    "test:e2e:chrome-mcp:validate": "node scripts/validate-chrome-mcp-config.cjs"
  },
  "dependencies": {
    "@cloudbase/js-sdk": "^2.15.1",
    "finalhandler": "^1.2.0",
    "lucide-react": "^0.395.0",
    "mime": "^3.0.0",
    "qrcode": "^1.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.26.2",
    "serve-static": "^1.15.0",
    "tcb-admin-node": "^1.23.0",
    "tldts": "^6.0.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.2",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.4.5",
    "jsdom": "^27.0.0",
    "npm-run-all": "^4.1.5",
    "pkg": "^5.8.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.5.4",
    "vite": "^5.4.8",
    "vitest": "^3.2.4"
  }
}
