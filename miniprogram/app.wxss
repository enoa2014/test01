page {
  min-height: 100vh;
  background: #f5f5f5;
  font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, "Noto Sans", sans-serif;
  color: #1f2933;
}

button {
  background-color: #07c160;
  color: #ffffff;
  border: none;
  border-radius: 8rpx;
  padding: 20rpx 32rpx;
}

button::after {
  border: none;
}